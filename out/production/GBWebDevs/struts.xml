<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
    <package name="default" extends="struts-default">
        <action name="index">
            <result>index.jsp</result>
        </action>
        <action name="product" class="pe.com.fueradelgarage.actions.ProductAction">
            <result name="success">showUser.jsp</result>
            <result name="input">listUsers.jsp</result>
        </action>
        <action name="user" class="pe.com.fueradelgarage.models.GbService">
            <result name="success">listUsers.jsp</result>
            <result name="show">showUser.jsp</result>
            <result name="new">newUser.jsp</result>
            <result name="edit">editUser.jsp</result>
            <result name="delete">listUsers.jsp</result>

        </action>
    </package>
</struts>